# Data Hub Configuration

# Cache settings
CACHE_TTL=600  # Cache TTL in seconds (10 minutes)
DATABASE_URL=sqlite+aiosqlite:///./data_hub.db

# Provider settings
DEFAULT_EXCHANGE=binance
PROVIDER_TIMEOUT=30  # Timeout in seconds

# API settings
API_TITLE="Data Hub API"
API_VERSION="0.1.0"
API_DESCRIPTION="FastAPI service for OHLCV data with caching"

# Logging
LOG_LEVEL=INFO

# CORS settings (comma-separated for lists)
CORS_ORIGINS=*
CORS_CREDENTIALS=true
CORS_METHODS=*
CORS_HEADERS=*
